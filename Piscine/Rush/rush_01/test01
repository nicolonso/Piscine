#include <unistd.h>
#include <stdlib.h>

char 	read_value(char *str)
{
	int	i;

	i = 0;
	while (str[i])
	{
		if (str[i] >= 1 && str[i] <= 4)
		{
			i++;
		}
		else
		{
			return (1);
		}
	return (0);
}

